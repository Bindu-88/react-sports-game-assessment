    name={this.props.visitingTeam.name}\r\n            logo={this.props.visitingTeam.logoSrc}\r\n            stats={this.state.visitingTeamStats}\r\n            shotHandler={() => this.shoot(\"visiting\")}\r\n          />\r\n\r\n          <div className=\"versus\">\r\n            <h1>VS</h1>\r\n            <div>\r\n              <strong>Resets:</strong> {this.state.resetCount}\r\n              <button onClick={this.resetGame}>Reset Game</button>\r\n            </div>\r\n          </div>\r\n\r\n          <Team\r\n            name={this.props.homeTeam.name}\r\n            logo={this.props.homeTeam.logoSrc}\r\n            stats={this.state.homeTeamStats}\r\n            shotHandler={() => this.shoot(\"home\")}\r\n          />\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Game;\r\n","C:\\Users\\pvira\\kenzie-projects\\react-sports-game-assessment\\src\\Components\\scoreboard\\ScoreBoard.js",["37"],"import React, { Components } from \"react\";\r\nfunction ScoreBoard(props) {\r\n  return (\r\n    <div className=\"ScoreBoard\">\r\n      <div className=\"teamStats\">\r\n        <h3>VISITORS</h3>\r\n        <h3>{props.visitingTeamStats.score}</h3>\r\n      </div>\r\n\r\n      <h3>SCOREBOARD</h3>\r\n\r\n      <div className=\"teamStats\">\r\n        <h3>HOME</h3>\r\n        <h3>{props.homeTeamStats.score}</h3>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\nexport default ScoreBoard;\r\n",{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","severity":1,"message":"43","line":2,"column":8,"nodeType":"44","messageId":"45","endLine":2,"endColumn":12},{"ruleId":"42","severity":1,"message":"46","line":4,"column":8,"nodeType":"44","messageId":"45","endLine":4,"endColumn":18},{"ruleId":"42","severity":1,"message":"47","line":1,"column":17,"nodeType":"44","messageId":"45","endLine":1,"endColumn":27},{"ruleId":"42","severity":1,"message":"47","line":1,"column":17,"nodeType":"44","messageId":"45","endLine":1,"endColumn":27},{"r